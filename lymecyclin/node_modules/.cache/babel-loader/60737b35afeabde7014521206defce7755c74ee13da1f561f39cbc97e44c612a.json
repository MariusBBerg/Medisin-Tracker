{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PillsCalendar from \"./PillsCalendar.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    PillsCalendar\n  },\n  data() {\n    return {\n      username: \"\",\n      lastMilkTime: \"\",\n      lastPillTime: \"\",\n      canDrinkMilk: false,\n      canTakePill: false,\n      pillCountdown: \"\",\n      milkCountdown: \"\"\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return this.$store.state.isLoggedIn;\n    }\n  },\n  async created() {\n    if (this.isLoggedIn) {\n      try {\n        let response = await this.$axios.get(\"/\");\n        this.username = response.data.username;\n        this.checkStatus();\n        this.startCountdowns();\n      } catch (error) {\n        console.error(\"Failed to get user:\", error);\n      }\n    }\n  },\n  methods: {\n    async logOut() {\n      console.log(this.$store.state.isLoggedIn);\n\n      // Check if the user is logged in\n      if (this.$store.state.isLoggedIn) {\n        try {\n          // Log out the user on the server\n          await this.$axios.post(\"/logout\");\n\n          // Log out the user on the client\n          this.$store.dispatch(\"logout\");\n          this.$router.push(\"/login\");\n        } catch (error) {\n          console.error(\"Failed to log out:\", error);\n        }\n      } else {\n        console.log(\"User is not logged in\");\n      }\n    },\n    async drinkMilk() {\n      if (!this.canDrinkMilk) {\n        alert(\"Cannot drink milk since you have taken a pill\");\n        return;\n      } else {\n        try {\n          await this.$axios.post(\"/drink_milk\");\n          alert(response.data.message);\n          this.checkStatus();\n        } catch (error) {\n          console.error(\"Failed to log milk:\", error);\n          alert(error.response.data.message);\n        }\n      }\n    },\n    async takePill() {\n      if (!this.canTakePill) {\n        alert(\"Cannot take pill less than 5 hours apart\");\n        return;\n      }\n      try {\n        await this.$axios.post(\"/take_pill\");\n        this.checkStatus();\n      } catch (error) {\n        console.error(\"Failed to log pill:\", error);\n      }\n    },\n    async checkStatus() {\n      try {\n        let response = await this.$axios.get(\"/can_i\");\n        this.lastMilkTime = response.data.last_milk_time;\n        this.lastPillTime = response.data.last_pill_time;\n        this.canDrinkMilk = response.data.can_drink_milk;\n        this.canTakePill = response.data.can_take_pill;\n      } catch (error) {\n        console.error(\"Failed to check status:\", error);\n      }\n    },\n    startCountdowns() {\n      setInterval(() => {\n        const now = new Date().getTime();\n        if (this.lastPillTime) {\n          const lastPillTime = new Date(this.lastPillTime).getTime();\n          // For melk: 1 time etter pille\n          const milkDiff = lastPillTime + 3600000 - now; // 1 time i millisekunder\n          this.milkCountdown = this.formatTime(milkDiff);\n\n          // For piller: 5 timer etter siste pille, men også vurder 1 time etter melk\n          const lastMilkTime = this.lastMilkTime ? new Date(this.lastMilkTime).getTime() : 0;\n          const pillDiff = Math.max(lastPillTime + 18000000, lastMilkTime + 3600000) - now; // 5 timer og 1 time i millisekunder\n          this.pillCountdown = this.formatTime(pillDiff);\n        } else {\n          // Hvis ingen pille er tatt ennå, tillat å drikke melk og ta pille \"nå\"\n          if (!this.lastMilkTime) {\n            this.milkCountdown = \"Now\";\n          }\n          this.pillCountdown = \"Now\";\n        }\n      }, 1000);\n    },\n    formatTime(milliseconds) {\n      if (milliseconds < 0) {\n        return \"Now\";\n      }\n      const seconds = Math.floor(milliseconds / 1000);\n      const minutes = Math.floor(seconds / 60);\n      const hours = Math.floor(minutes / 60);\n      return `${hours} hours, ${minutes % 60} minutes, ${seconds % 60} seconds`;\n    }\n  }\n};","map":{"version":3,"names":["PillsCalendar","name","components","data","username","lastMilkTime","lastPillTime","canDrinkMilk","canTakePill","pillCountdown","milkCountdown","computed","isLoggedIn","$store","state","created","response","$axios","get","checkStatus","startCountdowns","error","console","methods","logOut","log","post","dispatch","$router","push","drinkMilk","alert","message","takePill","last_milk_time","last_pill_time","can_drink_milk","can_take_pill","setInterval","now","Date","getTime","milkDiff","formatTime","pillDiff","Math","max","milliseconds","seconds","floor","minutes","hours"],"sources":["C:\\Users\\mariu\\Documents\\Kode\\Prosjekter\\melk\\lymecyclin\\src\\components\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <img src=\"../assets/logo.png\" class=\"logo\" />\r\n    <h1 v-if=\"isLoggedIn\">Hello {{ username }}</h1>\r\n    <div v-if=\"!isLoggedIn\">\r\n      <router-link to=\"/sign-up\">\r\n        <button class=\"action-button\">Sign Up</button>\r\n      </router-link>\r\n      <router-link to=\"/login\">\r\n        <button class=\"action-button\">Log In</button>\r\n      </router-link>\r\n    </div>\r\n    <div v-if=\"isLoggedIn\">\r\n      <div class=\"actions\">\r\n        <button class=\"action-button\" @click=\"drinkMilk\">Drink Milk</button>\r\n        <button class=\"action-button\" @click=\"takePill\">Take Pill</button>\r\n      </div>\r\n      <div class=\"status\">\r\n        <p>Last milk time: {{ lastMilkTime }}</p>\r\n        <p>Last pill time: {{ lastPillTime }}</p>\r\n        <p>Can drink milk: {{ canDrinkMilk }}</p>\r\n        <p>Can take pill: {{ canTakePill }}</p>\r\n        <p>Time until next milk: {{ milkCountdown }}</p>\r\n        <p>Time until next pill: {{ pillCountdown }}</p>\r\n      </div>\r\n\r\n      <PillsCalendar />\r\n\r\n      <button class=\"logout-button\" @click=\"logOut\">Log Out</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PillsCalendar from \"./PillsCalendar.vue\";\r\n\r\nexport default {\r\n  name: \"HomeView\",\r\n\r\n  components: {\r\n    PillsCalendar,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      lastMilkTime: \"\",\r\n      lastPillTime: \"\",\r\n      canDrinkMilk: false,\r\n      canTakePill: false,\r\n      pillCountdown: \"\",\r\n      milkCountdown: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      return this.$store.state.isLoggedIn;\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    if (this.isLoggedIn) {\r\n      try {\r\n        let response = await this.$axios.get(\"/\");\r\n        this.username = response.data.username;\r\n        this.checkStatus();\r\n        this.startCountdowns();\r\n      } catch (error) {\r\n        console.error(\"Failed to get user:\", error);\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async logOut() {\r\n      console.log(this.$store.state.isLoggedIn);\r\n\r\n      // Check if the user is logged in\r\n      if (this.$store.state.isLoggedIn) {\r\n        try {\r\n          // Log out the user on the server\r\n          await this.$axios.post(\"/logout\");\r\n\r\n          // Log out the user on the client\r\n          this.$store.dispatch(\"logout\");\r\n          this.$router.push(\"/login\");\r\n        } catch (error) {\r\n          console.error(\"Failed to log out:\", error);\r\n        }\r\n      } else {\r\n        console.log(\"User is not logged in\");\r\n      }\r\n    },\r\n\r\n    async drinkMilk() {\r\n      if (!this.canDrinkMilk) {\r\n        alert(\"Cannot drink milk since you have taken a pill\");\r\n        return;\r\n      } else {\r\n        try {\r\n          await this.$axios.post(\"/drink_milk\");\r\n          alert(response.data.message);\r\n          this.checkStatus();\r\n        } catch (error) {\r\n          console.error(\"Failed to log milk:\", error);\r\n          alert(error.response.data.message);\r\n\r\n        }\r\n      }\r\n    },\r\n\r\n    async takePill() {\r\n      if (!this.canTakePill) {\r\n        alert(\"Cannot take pill less than 5 hours apart\");\r\n        return;\r\n      }\r\n      try {\r\n        await this.$axios.post(\"/take_pill\");\r\n        this.checkStatus();\r\n      } catch (error) {\r\n        console.error(\"Failed to log pill:\", error);\r\n      }\r\n    },\r\n\r\n    async checkStatus() {\r\n      try {\r\n        let response = await this.$axios.get(\"/can_i\");\r\n        this.lastMilkTime = response.data.last_milk_time;\r\n        this.lastPillTime = response.data.last_pill_time;\r\n        this.canDrinkMilk = response.data.can_drink_milk;\r\n        this.canTakePill = response.data.can_take_pill;\r\n      } catch (error) {\r\n        console.error(\"Failed to check status:\", error);\r\n      }\r\n    },\r\n\r\n    startCountdowns() {\r\n      setInterval(() => {\r\n        const now = new Date().getTime();\r\n\r\n        if (this.lastPillTime) {\r\n          const lastPillTime = new Date(this.lastPillTime).getTime();\r\n          // For melk: 1 time etter pille\r\n          const milkDiff = lastPillTime + 3600000 - now; // 1 time i millisekunder\r\n          this.milkCountdown = this.formatTime(milkDiff);\r\n\r\n          // For piller: 5 timer etter siste pille, men også vurder 1 time etter melk\r\n          const lastMilkTime = this.lastMilkTime\r\n            ? new Date(this.lastMilkTime).getTime()\r\n            : 0;\r\n          const pillDiff =\r\n            Math.max(lastPillTime + 18000000, lastMilkTime + 3600000) - now; // 5 timer og 1 time i millisekunder\r\n          this.pillCountdown = this.formatTime(pillDiff);\r\n        } else {\r\n          // Hvis ingen pille er tatt ennå, tillat å drikke melk og ta pille \"nå\"\r\n          if (!this.lastMilkTime) {\r\n            this.milkCountdown = \"Now\";\r\n          }\r\n          this.pillCountdown = \"Now\";\r\n        }\r\n      }, 1000);\r\n    },\r\n\r\n    formatTime(milliseconds) {\r\n      if (milliseconds < 0) {\r\n        return \"Now\";\r\n      }\r\n      const seconds = Math.floor(milliseconds / 1000);\r\n      const minutes = Math.floor(seconds / 60);\r\n      const hours = Math.floor(minutes / 60);\r\n      return `${hours} hours, ${minutes % 60} minutes, ${seconds % 60} seconds`;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.logo {\r\n  width: 100px;\r\n}\r\n.home {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  padding: 20px;\r\n}\r\n\r\n.home p {\r\n  width: 300px;\r\n  padding: 10px;\r\n  border: 1px solid #ea5736;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.action-button {\r\n  width: 300px;\r\n  height: 40px;\r\n  background-color: #ea5736;\r\n  border: 1px #ea5736;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.logout-button {\r\n  width: 300px;\r\n  height: 40px;\r\n  background-color: #fff;\r\n  border: 1px #ea5736;\r\n  color: #ea5736;\r\n  cursor: pointer;\r\n  margin-top: 10px;\r\n}\r\n\r\n.actions {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.status {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.status p {\r\n  width: 300px;\r\n  padding: 10px;\r\n  border: 1px solid #ea5736;\r\n  margin-bottom: 10px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"mappings":";AAkCA,OAAOA,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAEhBC,UAAU,EAAE;IACVF;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,KAAK;MACnBC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,EAAE;MACjBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU;IACrC;EACF,CAAC;EAED,MAAMG,OAAOA,CAAA,EAAG;IACd,IAAI,IAAI,CAACH,UAAU,EAAE;MACnB,IAAI;QACF,IAAII,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,GAAG,CAAC,GAAG,CAAC;QACzC,IAAI,CAACd,QAAO,GAAIY,QAAQ,C