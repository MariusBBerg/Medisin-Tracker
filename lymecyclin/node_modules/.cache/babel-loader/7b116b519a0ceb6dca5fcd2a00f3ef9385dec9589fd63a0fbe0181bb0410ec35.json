{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport VueCal from 'vue-cal';\nimport 'vue-cal/dist/vuecal.css';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      username: '',\n      lastMilkTime: '',\n      lastPillTime: '',\n      canDrinkMilk: false,\n      canTakePill: false,\n      twoPillsDays: []\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return this.$store.state.isLoggedIn;\n    }\n  },\n  async created() {\n    if (this.isLoggedIn) {\n      try {\n        let response = await this.$axios.get('/');\n        this.username = response.data.username;\n        this.getTwoPillsDays();\n        this.checkStatus();\n      } catch (error) {\n        console.error('Failed to get user:', error);\n      }\n    }\n  },\n  methods: {\n    async logOut() {\n      console.log(this.$store.state.isLoggedIn);\n\n      // Check if the user is logged in\n      if (this.$store.state.isLoggedIn) {\n        try {\n          // Log out the user on the server\n          await this.$axios.post('/logout');\n\n          // Log out the user on the client\n          this.$store.dispatch('logout');\n          this.$router.push('/login');\n        } catch (error) {\n          console.error('Failed to log out:', error);\n        }\n      } else {\n        console.log('User is not logged in');\n      }\n    },\n    async drinkMilk() {\n      try {\n        await this.$axios.post('/drink_milk');\n        this.checkStatus();\n      } catch (error) {\n        console.error('Failed to log milk:', error);\n      }\n    },\n    async takePill() {\n      try {\n        await this.$axios.post('/take_pill');\n        this.checkStatus();\n      } catch (error) {\n        console.error('Failed to log pill:', error);\n      }\n    },\n    async checkStatus() {\n      try {\n        let response = await this.$axios.get('/can_i');\n        this.lastMilkTime = response.data.last_milk_time;\n        this.lastPillTime = response.data.last_pill_time;\n        this.canDrinkMilk = response.data.can_drink_milk;\n        this.canTakePill = response.data.can_take_pill;\n      } catch (error) {\n        console.error('Failed to check status:', error);\n      }\n    },\n    async getTwoPillsDays() {\n      try {\n        let response = await this.$axios.get('/two_pills_days');\n        this.twoPillsDays = response.data.two_pills_days;\n        console.log(this.twoPillsDays);\n      } catch (error) {\n        console.error('Failed to get two pills days:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["VueCal","name","data","username","lastMilkTime","lastPillTime","canDrinkMilk","canTakePill","twoPillsDays","computed","isLoggedIn","$store","state","created","response","$axios","get","getTwoPillsDays","checkStatus","error","console","methods","logOut","log","post","dispatch","$router","push","drinkMilk","takePill","last_milk_time","last_pill_time","can_drink_milk","can_take_pill","two_pills_days"],"sources":["C:\\Users\\mariu\\Documents\\Kode\\Prosjekter\\melk\\lymecyclin\\src\\components\\Home.vue"],"sourcesContent":["<template>\r\n    <div class=\"home\">\r\n        <img src=\"../assets/logo.png\" class=\"logo\">\r\n        <h1 v-if=\"isLoggedIn\">Hello {{ username }} </h1>\r\n        <div v-if=\"!isLoggedIn\">\r\n            <router-link to=\"/sign-up\">\r\n                <button class=\"action-button\">Sign Up</button>\r\n            </router-link>\r\n            <router-link to=\"/login\">\r\n                <button class=\"action-button\">Log In</button>\r\n            </router-link>\r\n        </div>\r\n        <div v-if=\"isLoggedIn\">\r\n            <div class=\"actions\">\r\n                <button class=\"action-button\" @click=\"drinkMilk\">Drink Milk</button>\r\n                <button class=\"action-button\" @click=\"takePill\">Take Pill</button>\r\n            </div>\r\n            <div class=\"status\">\r\n                <p>Last milk time: {{ lastMilkTime }}</p>\r\n                <p>Last pill time: {{ lastPillTime }}</p>\r\n                <p>Can drink milk: {{ canDrinkMilk }}</p>\r\n                <p>Can take pill: {{ canTakePill }}</p>\r\n            </div>\r\n            <div class=\"calendar-container\" v-if=\"isLoggedIn\">\r\n                <vue-cal :events=\"pillEvents\" :on-event-click=\"eventClicked\"></vue-cal>\r\n            </div>\r\n\r\n            <div class=\"two-pills-days\">\r\n                <h2>Days with two pills:</h2>\r\n                <ul>\r\n                    <li v-for=\"day in twoPillsDays\" :key=\"day\">{{ day }}</li>\r\n                </ul>\r\n            </div>\r\n\r\n            <button class=\"logout-button\" @click=\"logOut\">Log Out</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport VueCal from 'vue-cal';\r\nimport 'vue-cal/dist/vuecal.css';\r\n\r\n\r\nexport default{\r\n    name: 'HomeView',\r\n\r\n    data() {\r\n        return {\r\n            username: '',\r\n            lastMilkTime: '',\r\n            lastPillTime: '',\r\n            canDrinkMilk: false,\r\n            canTakePill: false,\r\n            twoPillsDays: [],\r\n        };\r\n    },\r\n    computed: {\r\n        isLoggedIn() {\r\n            return this.$store.state.isLoggedIn;\r\n        },\r\n    },\r\n\r\n    async created() {\r\n    if (this.isLoggedIn) {\r\n        try {\r\n            let response = await this.$axios.get('/');\r\n            this.username = response.data.username;\r\n            this.getTwoPillsDays();\r\n            this.checkStatus();\r\n        } catch (error) {\r\n            console.error('Failed to get user:', error);\r\n        }\r\n    }\r\n},\r\n\r\n    methods: {\r\n        async logOut() {\r\n            console.log(this.$store.state.isLoggedIn);\r\n\r\n            // Check if the user is logged in\r\n            if (this.$store.state.isLoggedIn) {\r\n                try {\r\n                    // Log out the user on the server\r\n                    await this.$axios.post('/logout');\r\n\r\n                    // Log out the user on the client\r\n                    this.$store.dispatch('logout');\r\n                    this.$router.push('/login');\r\n                } catch (error) {\r\n                    console.error('Failed to log out:', error);\r\n                }\r\n            } else {\r\n                console.log('User is not logged in');\r\n            }\r\n        },\r\n\r\n        async drinkMilk() {\r\n            try {\r\n                await this.$axios.post('/drink_milk');\r\n                this.checkStatus();\r\n            } catch (error) {\r\n                console.error('Failed to log milk:', error);\r\n            }\r\n        },\r\n\r\n        async takePill() {\r\n            try {\r\n                await this.$axios.post('/take_pill');\r\n                this.checkStatus();\r\n            } catch (error) {\r\n                console.error('Failed to log pill:', error);\r\n            }\r\n        },\r\n\r\n        async checkStatus() {\r\n            try {\r\n                let response = await this.$axios.get('/can_i');\r\n                this.lastMilkTime = response.data.last_milk_time;\r\n                this.lastPillTime = response.data.last_pill_time;\r\n                this.canDrinkMilk = response.data.can_drink_milk;\r\n                this.canTakePill = response.data.can_take_pill;\r\n            } catch (error) {\r\n                console.error('Failed to check status:', error);\r\n            }\r\n        },\r\n        async getTwoPillsDays(){\r\n            try {\r\n                let response = await this.$axios.get('/two_pills_days');\r\n                this.twoPillsDays = response.data.two_pills_days;\r\n                console.log(this.twoPillsDays)\r\n            } catch (error) {\r\n                console.error('Failed to get two pills days:', error);\r\n            }\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.logo{\r\n    width: 100px;   \r\n}\r\n.home {\r\n    display:flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding: 20px;\r\n}\r\n\r\n.home p {\r\n    width: 300px;\r\n    padding: 10px;\r\n    border: 1px solid #ea5736;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.action-button {\r\n    width: 300px;\r\n    height: 40px;\r\n    background-color: #ea5736;\r\n    border: 1px #ea5736;\r\n    color: #fff;\r\n    cursor: pointer;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.logout-button {\r\n    width: 300px;\r\n    height: 40px;\r\n    background-color: #fff;\r\n    border: 1px #ea5736;\r\n    color: #ea5736;\r\n    cursor: pointer;\r\n    margin-top: 10px;\r\n}\r\n\r\n.actions {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.status {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.status p {\r\n    width: 300px;\r\n    padding: 10px;\r\n    border: 1px solid #ea5736;\r\n    margin-bottom: 10px;\r\n    text-align: center;\r\n}\r\n</style>"],"mappings":";AAwCA,OAAOA,MAAK,MAAO,SAAS;AAC5B,OAAO,yBAAyB;AAGhC,eAAc;EACVC,IAAI,EAAE,UAAU;EAEhBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,KAAK;MACnBC,WAAW,EAAE,KAAK;MAClBC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,UAAUA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU;IACvC;EACJ,CAAC;EAED,MAAMG,OAAOA,CAAA,EAAG;IAChB,IAAI,IAAI,CAACH,UAAU,EAAE;MACjB,IAAI;QACA,IAAII,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,GAAG,CAAC,GAAG,CAAC;QACzC,IAAI,CAACb,QAAO,GAAIW,QAAQ,CAACZ,IAAI,CAACC,QAAQ;QACtC,IAAI,CAACc,eAAe,CAAC,CAAC;QACtB,IAAI,CAACC,WAAW,CAAC,CAAC;MACtB,EAAE,OAAOC,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;MAC/C;IACJ;EACJ,CAAC;EAEGE,OAAO,EAAE;IACL,MAAMC,MAAMA,CAAA,EAAG;MACXF,OAAO,CAACG,GAAG,CAAC,IAAI,CAACZ,MAAM,CAACC,KAAK,CAACF,UAAU,CAAC;;MAEzC;MACA,IAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU,EAAE;QAC9B,IAAI;UACA;UACA,MAAM,IAAI,CAACK,MAAM,CAACS,I