PillTime: '',\r\n            canDrinkMilk: false,\r\n            canTakePill: false,\r\n            twoPillsDays: [],\r\n            pillEvents: [],\r\n        };\r\n    },\r\n    computed: {\r\n        isLoggedIn() {\r\n            return this.$store.state.isLoggedIn;\r\n        },\r\n    },\r\n\r\n    async created() {\r\n    if (this.isLoggedIn) {\r\n        try {\r\n            let response = await this.$axios.get('/');\r\n            this.username = response.data.username;\r\n            this.getTwoPillsDays();\r\n            this.checkStatus();\r\n        } catch (error) {\r\n            console.error('Failed to get user:', error);\r\n        }\r\n    }\r\n},\r\n\r\n    methods: {\r\n        async logOut() {\r\n            console.log(this.$store.state.isLoggedIn);\r\n\r\n            // Check if the user is logged in\r\n            if (this.$store.state.isLoggedIn) {\r\n                try {\r\n                    // Log out the user on the server\r\n                    await this.$axios.post('/logout');\r\n\r\n                    // Log out the user on the client\r\n                    this.$store.dispatch('logout');\r\n                    this.$router.push('/login');\r\n                } catch (error) {\r\n                    console.error('Failed to log out:', error);\r\n                }\r\n            } else {\r\n                console.log('User is not logged in');\r\n            }\r\n        },\r\n\r\n        async drinkMilk() {\r\n            try {\r\n                await this.$axios.post('/drink_milk');\r\n                this.checkStatus();\r\n            } catch (error) {\r\n                console.error('Failed to log milk:', error);\r\n            }\r\n        },\r\n\r\n        async takePill() {\r\n            try {\r\n                await this.$axios.post('/take_pill');\r\n                this.checkStatus();\r\n            } catch (error) {\r\n                console.error('Failed to log pill:', error);\r\n            }\r\n        },\r\n\r\n        async checkStatus() {\r\n            try {\r\n                let response = await this.$axios.get('/can_i');\r\n                this.lastMilkTime = response.data.last_milk_time;\r\n                this.lastPillTime = response.data.last_pill_time;\r\n                this.canDrinkMilk = response.data.can_drink_milk;\r\n                this.canTakePill = response.data.can_take_pill;\r\n            } catch (error) {\r\n                console.error('Failed to check status:', error);\r\n            }\r\n        },\r\n        async getTwoPillsDays(){\r\n            try {\r\n                let response = await this.$axios.get('/two_pills_days');\r\n                this.twoPillsDays = response.data.two_pills_days;\r\n                console.log(this.twoPillsDays)\r\n            } catch (error) {\r\n                console.error('Failed to get two pills days:', error);\r\n            }\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.logo{\r\n    width: 100px;   \r\n}\r\n.home {\r\n    display:flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding: 20px;\r\n}\r\n\r\n.home p {\r\n    width: 300px;\r\n    padding: 10px;\r\n    border: 1px solid #ea5736;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.action-button {\r\n    width: 300px;\r\n    height: 40px;\r\n    background-color: #ea5736;\r\n    border: 1px #ea5736;\r\n    color: #fff;\r\n    cursor: pointer;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.logout-button {\r\n    width: 300px;\r\n    height: 40px;\r\n    background-color: #fff;\r\n    border: 1px #ea5736;\r\n    color: #ea5736;\r\n    cursor: pointer;\r\n    margin-top: 10px;\r\n}\r\n\r\n.actions {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.status {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.status p {\r\n    width: 300px;\r\n    padding: 10px;\r\n    border: 1px solid #ea5736;\r\n    margin-bottom: 10px;\r\n    text-align: center;\r\n}\r\n</style>"],"mappings":";OAEaA,UAAwB;;EAD5BC,KAAK,EAAC;AAAM;gCACbC,mBAAA,CAA2C;EAAtCC,GAAwB,EAAxBH,UAAwB;EAACC,KAAK,EAAC;;;EAF5CG,GAAA;AAAA;;EAAAA,GAAA;AAAA;gCAMgBF,mBAAA,CAA8C;EAAtCD,KAAK,EAAC;AAAe,GAAC,SAAO;gCAGrCC,mBAAA,CAA6C;EAArCD,KAAK,EAAC;AAAe,GAAC,QAAM;;EATpDG,GAAA;AAAA;;EAaiBH,KAAK,EAAC;AAAS;;EAIfA,KAAK,EAAC;AAAQ;;EAQdA,KAAK,EAAC;AAAgB;iCACvBC,mBAAA,CAA6B,YAAzB,sBAAoB;;;uBAzBpCG,mBAAA,CAiCM,OAjCNC,UAiCM,GAhCFC,UAA2C,EACjCC,QAAA,CAAAC,UAAU,I,cAApBJ,mBAAA,CAAgD,MAHxDK,UAAA,EAG8B,QAAM,GAAAC,gBAAA,CAAGC,KAAA,CAAAC,QAAQ,oBAH/CC,mBAAA,gB,CAIoBN,QAAA,CAAAC,UAAU,I,cAAtBJ,mBAAA,CAOM,OAXdU,UAAA,GAKYC,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAU;IALtCC,OAAA,EAAAC,QAAA,CAMgB,MAA8C,CAA9CC,UAA8C,C;IAN9DC,CAAA;MAQYN,YAAA,CAEcC,sBAAA;IAFDC,EAAE,EAAC;EAAQ;IARpCC,OAAA,EAAAC,QAAA,CASgB,MAA6C,CAA7CG,UAA6C,C;IAT7DD,CAAA;UAAAR,mBAAA,gBAYmBN,QAAA,CAAAC,UAAU,I,cAArBJ,mBAAA,CAqBM,OAjCdmB,UAAA,GAaYtB,mBAAA,CAGM,OAHNuB,UAGM,GAFFvB,mBAAA,CAAoE;IAA5DD,KAAK,EAAC,eAAe;IAAEyB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEpB,QAAA,CAAAqB,SAAA,IAAArB,QAAA,CAAAqB,SAAA,IAAAD,IAAA,CAAS;KAAE,YAAU,GAC3D1B,mBAAA,CAAkE;IAA1DD,KAAK,EAAC,eAAe;IAAEyB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEpB,QAAA,CAAAsB,QAAA,IAAAtB,QAAA,CAAAsB,QAAA,IAAAF,IAAA,CAAQ;KAAE,WAAS,E,GAE7D1B,mBAAA,CAKM,OALN6B,UAKM,GAJF7B,mBAAA,CAAyC,WAAtC,kBAAgB,GAAAS,gBAAA,CAAGC,KAAA,CAAAoB,YAAY,kBAClC9B,mBAAA,CAAyC,WAAtC,kBAAgB,GAAAS,gBAAA,CAAGC,KAAA,CAAAqB,YAAY,kBAClC/B,mBAAA,CAAyC,WAAtC,kBAAgB,GAAAS,gBAAA,CAAGC,KAAA,CAAAsB,YAAY,kBAClChC,mBAAA,CAAuC,WAApC,iBAAe,GAAAS,gBAAA,CAAGC,KAAA,CAAAuB,WAAW,iB,GAIpCjC,mBAAA,CAKM,OALNkC,WAKM,GAJFC,WAA6B,EAC7BnC,mBAAA,CAEK,c,kBADDG,mBAAA,CAAyDiC,SAAA,QA5B7EC,WAAA,CA4BsC3B,KAAA,CAAA4B,YAAY,EAAnBC,GAAG;yBAAdpC,mBAAA,CAAyD;MAAxBD,GAAG,EAAEqC;IAAG,GAAA9B,gBAAA,CAAK8B,GAAG;sCAIzDvC,mBAAA,CAA8D;IAAtDD,KAAK,EAAC,eAAe;IAAEyB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEpB,QAAA,CAAAkC,MAAA,IAAAlC,QAAA,CAAAkC,MAAA,IAAAd,IAAA,CAAM;KAAE,SAAO,E,KAhCjEd,mBAAA,e"}